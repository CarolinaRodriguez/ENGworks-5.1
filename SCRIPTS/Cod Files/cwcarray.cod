requires task.selection

dim item1 = task.selection[1]

dim MPSS = inputbox("Enter MPSS Number or Click OK", "FAB Batch" , item1.customdata["FAB Batch"].value)
dim POCS = inputbox("Enter PO Number for Carbon Steel Material")
dim i = 1

while i <= task.selection.count
    dim item = task.selection[i]
if item.material = "BD Piping Material: Carbon Steel" then
    item.customdata["FAB Batch"].value = MPSS
    item.customdata["Purchase Order Number"].value = POCS
    item.update()
endif
 
    i = i + 1
end while