
requires task.selection
dim item1 = task.selection[1]

Dim tnumber = inputbox("Enter Terminal Number or Click OK" , "Terminal Number" , item1.customdata["Terminal Number"].value)

dim i = 1

while i <= task.selection.count
    dim item = task.selection[i]
    item.customdata["Terminal Number"].value = tnumber
    item.update()
 
    i = i + 1
end while
