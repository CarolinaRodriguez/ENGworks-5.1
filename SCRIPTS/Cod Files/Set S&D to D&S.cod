





If item.connector[1].type = "Rectangular" and item.cid <> 12 then
dim lp
for lp = 1 to item.dims
  If Item.dim[lp].value > Item.dim["Width"].value or Item.dim["Top Width"].value Then
    If Item.connectors > 0 then
      Dim Conn1 = item.connector[1].value
        if (wildcard(Conn1,"*S&D.")) or (wildcard(Conn1,"*S&FD.")) then
         item.connector[1].value = "D&S."
         Item.Connector[1].locked = true
        endif
     Endif
    If Item.connectors > 1 then
      Dim Conn2 = item.connector[2].value
        if (wildcard(Conn2,"*S&D.")) or (wildcard(Conn2,"*S&FD.")) then
         item.connector[2].value = "D&S."
         Item.Connector[2].locked = true
        endif
     Endif
    If Item.connectors > 2 then
      Dim Conn3 = item.connector[1].value
        if (wildcard(Conn3,"*S&D.")) or (wildcard(Conn3,"*S&FD.")) then
         item.connector[3].value = "D&S."
         Item.Connector[3].locked = true
        endif
     Endif
  endif
endif
Item.update()


  