function ChangeRodLength()
    select item.CID
        case 838
            dim i
            for i =  1 to item.dims
                select item.Dim[i].Name
                    case "Rod Length", "Rod Extn Above"
                        item.Dim[i].Value = RoundupLength(item.Dim[i].Value)
                end select
            next
        end select
end function

function RoundupLength(length)
    return roundup(length /  12) *  12
end function

ChangeRodLength()

