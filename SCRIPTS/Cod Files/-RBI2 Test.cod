Dim Pat = item.cid

Rem Change TDC End Cap Flange
If pat = 8621 then
 item.dim["Flange"].value = "1.625"
Endif

Rem Change Vee Notch for New Finger Break
If pat=866 and item.cuttype = "Machine Cut" then
Dim Con1 = item.connector[1].value
Dim Con2 = item.connector[2].value

  if (Wildcard(Con1, "*TDC*")) Then
     Item.option["Connector Fold Notch #1"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #1"].value = "10"
     item.option["Vee Notch Depth #1"].value = "1.88"
  endif
  if (Wildcard(Con2, "*TDC*")) Then
     Item.option["Connector Fold Notch #2"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #2"].value = "10"
     item.option["Vee Notch Depth #2"].value = "1.88"
  endif
  if (Wildcard(Con1, "*S&*")) or (Wildcard(Con1, "*D&*")) or (Wildcard(Con1, "*S+S*")) Then
     Item.option["Connector Fold Notch #1"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #1"].value = "30"
     item.option["Vee Notch Depth #1"].value = ".5"
  endif
  if (Wildcard(Con2, "*S&*")) or (Wildcard(Con2, "*D&*")) or (Wildcard(Con2, "*S+S*")) Then
     Item.option["Connector Fold Notch #2"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #2"].value = "30"
     item.option["Vee Notch Depth #2"].value = ".5"
  endif
Endif