Dim Pat = item.cid


Rem Debug item.option["Seam Number For Throat"].value

If item.cid = 8 then
   Item.seam[1].value = "1/2 Lap."
   Item.seam[1].locked = true
Endif

If item.cid = 20 then
 item.option["Seam Number For Throat"].value = 0
 Item.seam[1].value = "L-Pits."
 Item.seam[1].locked = true
Endif

If item.seams > 0 then
 If Item.seam[1].value = "S-Pits." then
 Item.seam[1].value = "L-Pits."
 Item.seam[1].locked = true
 endif
Endif

If item.seams > 1 then
If Item.seam[2].value = "S-Pits." then
 Item.seam[2].value = "L-Pits."
 Item.seam[2].locked = true
Endif
Endif

If pat=6 or pat=7 or pat=11 or pat=13 or pat=15 or pat=25 or pat=472 or pat=328 then
     Item.seam[1].value = "L-Pits."
     Item.seam[1].locked = true
  endif
 


If item.cid = 8621 then
  if item.connector[1].value = "Ward J." then
     item.dim["Flange"].value = "1.625"
     item.dim[5].value = ".75"
     item.dim[6].value = ".45"
     item.dim[7].value = ".45"
     item.dim[8].value = ".88"
  Endif
  if item.connector[1].value = "TDC." then
     item.dim[3].value = "1.25"
     item.dim[5].value = ".75"
     item.dim[6].value = ".50"
     item.dim[7].value = ".50"
     item.dim[8].value = ".88375"
  Endif
Endif

 
If pat=2 or pat=866 Then
  If item.dim["Length"].numvalue < 10 then
    If item.seams > 0 then
    Item.seam[1].value = "L-Pits."
    Item.seam[1].locked = true
    Endif
  If item.seams > 1 then
    Item.seam[2].value = "L-Pits."
    Item.seam[2].locked = true
    Endif
  Endif
Endif

Rem ------- Change Decoiled Fab Rate for Sheared Duct-------
If item.cid = 866 and Item.cuttype = "Decoiled Straight" then
 if item.option["Override"].value = "4xI,M-F" then
   item.option["Override"].value = "4xI,M-M,F-F"
 Endif
  Item.option["Override Cut Type"].value = "Sheared"
rem Item.fabtable = "Rect. Duct (Sheared w TDC-Plasma)"
Endif


Item.update()


 