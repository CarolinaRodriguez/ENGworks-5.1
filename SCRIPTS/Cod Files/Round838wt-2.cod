






function ChangeRodLength()
    select item.CID
        case 838
            dim i
            for i =  1 to item.dims
                select item.Dim[i].Name
                    case "Rod Length", "Rod Extn Above"
                        item.Dim[i].Value = RoundLength(item.Dim[i].numValue)
                end select
            next
        end select
end function

function RoundLength(length)
    return round(length /  1.125) *  1.125
end function

ChangeRodLength()

