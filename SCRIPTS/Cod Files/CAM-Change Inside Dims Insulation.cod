if pat = 866 or pat = 8621 or pat = 12 or pat = 15 or pat = 25 then
 dim W = Item.dim["Width"].numvalue
 dim D = Item.dim["Depth"].numvalue
 if item.dimside = "Inside" then
  item.insulation.materiallock = true
  item.insspec = "none"
  Item.dim["Width"].value = W+(IG*2)
  Item.dim["Depth"].value = D+(IG*2)
  item.dimside = "Outside"
  Item.customdata["ScriptCode"].value = SC + "-" + "InsideDimsIns"
 endif
Endif

