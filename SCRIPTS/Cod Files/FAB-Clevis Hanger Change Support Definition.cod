REM Look for CID 838 with Option 1 set to Clevis Hanger and then set the Support.Value to 'FAB-Clevis Hanger'

REM debug item.option[1].value
REM debug item.support.value

if item.CID=838 then
  if item.option[1].value="Clevis Hanger" then
    item.support.value="FAB-Clevis Hanger SS/SV"
    item.support.qty=1
    Item.option["Add Rod Size"].value = "Yes"
    Item.option["Use Support Settings"].value = "Yes"
  endif
endif