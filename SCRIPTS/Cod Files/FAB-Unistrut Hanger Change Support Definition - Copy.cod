REM Look for CID 838 with Option 1 set to Profiled Bearer and then set the Support.Value to 'FAB-Unistrut Hanger'

REM debug item.option[1].value
REM debug item.support.value

if item.CID=838 then
  if item.option[1].value="Profiled Bearer" then
    item.support.value="FAB-Unistrut Hanger"
    item.support.qty=1
    Item.option["Add Rod Size"].value = "Yes"
    Item.option["Use Support Settings"].value = "Yes"
  endif
endif