Dim Pat = item.cid


Rem Debug item.option["Seam Number For Throat"].value

If item.cid = 20 then
 if item.wiregauge > 19 then
 item.option["Seam Number For Throat"].value = 0
 Item.seam[1].value = "S-Pits."
 Item.seam[1].locked = true
 endif
 if item.wiregauge < 19 then
 item.option["Seam Number For Throat"].value = 0
 Item.seam[1].value = "L-Pits."
 Item.seam[1].locked = true
 endif
Endif

If pat=2 or pat=6 or pat=11 or pat=13 or pat=15 or pat=472 or pat=328 then
  if item.wiregauge > 19 then
     Item.seam[1].value = "S-Pits."
     Item.seam[1].locked = true
  endif
  if item.wiregauge < 19 then
     Item.seam[1].value = "L-Pits."
     Item.seam[1].locked = true
  endif
 if item.seams = 2 then
  if item.wiregauge > 19 then
     Item.seam[2].value = "S-Pits."
     Item.seam[2].locked = true
  endif
  if item.wiregauge < 19 then
     Item.seam[2].value = "L-Pits."
     Item.seam[2].locked = true
  endif
 endif
Endif

If pat=25 and item.dim[3].numvalue < 12 then
  if item.wiregauge > 19 then
     Item.seam[1].value = "S-Pits."
     Item.seam[1].locked = true
  endif
  if item.wiregauge < 19 then
     Item.seam[1].value = "L-Pits."
     Item.seam[1].locked = true
  endif
 if item.seams = 2 then
  if item.wiregauge > 19 then
     Item.seam[2].value = "S-Pits."
     Item.seam[2].locked = true
  endif
  if item.wiregauge < 19 then
     Item.seam[2].value = "L-Pits."
     Item.seam[2].locked = true
  endif
 endif
