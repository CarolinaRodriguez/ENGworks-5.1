If pat=866 then
 if item.seam[1].value = "snaplock Screwed" or item.seam[1].value = "snaplock Screwed." then
     Item.seam[1].value = "S-Pits."
     Item.seam[1].locked = true
     Item.seam[2].value = "S-Pits."
     Item.seam[2].locked = true
 Endif
Endif

Rem Change Vee Notch for New Finger Break
If pat=866 and item.cuttype = "Machine Cut" then
Dim Con1 = item.connector[1].value
Dim Con2 = item.connector[2].value

  if (Wildcard(Con1, "*TDC*")) Then
     Item.option["Connector Fold Notch #1"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #1"].value = "15"
     item.option["Vee Notch Depth #1"].value = "1.88"
  endif
  if (Wildcard(Con2, "*TDC*")) Then
     Item.option["Connector Fold Notch #2"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #2"].value = "15"
     item.option["Vee Notch Depth #2"].value = "1.88"
  endif
  if (Wildcard(Con1, "*S&*")) or (Wildcard(Con1, "*D&*")) or (Wildcard(Con1, "*S+S*")) Then
     Item.option["Connector Fold Notch #1"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #1"].value = "30"
     item.option["Vee Notch Depth #1"].value = ".5"
  endif
  if (Wildcard(Con2, "*S&*")) or (Wildcard(Con2, "*D&*")) or (Wildcard(Con2, "*S+S*")) Then
     Item.option["Connector Fold Notch #2"].value = "Use Vee Notch"
     item.option["Vee Notch Angle #2"].value = "30"
     item.option["Vee Notch Depth #2"].value = ".5"
  endif
Endif

Rem Change TDC End Cap Flange
If pat = 8621 then
 item.dim["Flange"].value = "1.625"
Endif

Rem Set Square to Round Collars
If item.cid = 8 then
	if Item.connector[2].value = "SE Collar-3." Then
	Dim NewLength = Item.dim["Length"].numvalue + 1.25
    Item.dim["Length"].value = NewLength
    Item.connector[2].value = "SE Collar Sheared."
	Item.connector[2].locked = true
    Endif
 Endif

Rem ------Set Reducers to Bought Out-----
If item.cid = 51 then
   item.boughtout = True
endif
Item.update()

If item.cid = 51 then
  if item.dim[1].numvalue > 14 or item.dim[2].numvalue > 14 then
      item.boughtout = False
    if item.wiregauge > 20 then
       item.wiregauge = 22
    endif       
  Endif
  if item.dim[1].numvalue > item.dim[2].numvalue then
   Dim SizeCH = item.dim[1].numvalue - item.dim[2].numvalue
    if SizeCH > 2 then
      item.boughtout = False
      if item.wiregauge > 20 then
         item.wiregauge = 22
      endif 
    Endif
  Endif
  if item.dim[2].numvalue > item.dim[1].numvalue then
    Dim SizeCH = item.dim[2].numvalue - item.dim[1].numvalue
    if SizeCH > 2 then
      item.boughtout = False
      if item.wiregauge > 20 then
         item.wiregauge = 22
      endif 
    endif
  endif
Endif
Rem ------Set Reducers to Bought Out-----

Item.update()


 