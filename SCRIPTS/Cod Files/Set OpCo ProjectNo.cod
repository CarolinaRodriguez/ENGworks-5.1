Rem Select Items and Enter OpCo Project Number Example 206-1234

requires task.selection

Dim OpCoJobNo = inputbox("Enter OpCo Number and Project Number","Example 206-1234")

dim i = 1

while i <= task.selection.count
    dim item = task.selection[i]
    item.customdata["OpCo-JobNo"].value = OpCoJobNo
    item.update()
 
    i = i + 1
end while

Debug "Project Number Applied"
