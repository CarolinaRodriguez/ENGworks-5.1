rem debug item.insulation.status
Rem debug item.insulation.Gauge
rem Debug item.connector[2].value

Rem Dim Stretchout = (Item.dim["Width"].numvalue + Item.dim["Depth"].numvalue)*2
Rem Dim HalfStretchout = Item.dim["Width"].numvalue + Item.dim["Depth"].numvalue


Rem Change patten 7 (Tap)----------------------
if item.cid = 7 then
 if item.insulation.status = "Off" or item.insulation.status = "Outside" then
  if item.connector[2].value = "F1-Tap." then
   rem item.connector[2].alt = ""
   item.connector[2].value = "OT-.625."
   item.connector[2].locked = true
  Endif
 Endif
Endif

if item.cid = 7 then
 if item.insulation.status = "Inside" and item.insulation.Gauge > .1 then
  if item.connector[2].value = "F1-Tap." then
   rem item.connector[2].alt = ""
   item.connector[2].value = "IT-.75."
   item.connector[2].locked = true
  Endif
 Endif
Endif

if item.cid = 7 then
 if Item.dim["Length" or "Height"].numvalue < 12 then
    item.Stiffener[1].value = "None"
    item.Stiffener[1].locked = true
   if item.wiregauge < 22 Then
     item.wiregauge = 22
   endif
 Endif
Endif

Rem Change patten 15 (Angle Tap)-------------------------
if item.cid = 15 then
 if item.insulation.status = "Off" or item.insulation.status = "Outside" then
  if item.connector[2].value = "F1-Tap." then
   rem item.connector[2].alt = ""
   item.connector[2].value = "OT-.625."
   item.connector[2].locked = true
  Endif
 Endif
Endif

if item.cid = 15 then
 if item.insulation.status = "Inside" and item.insulation.Gauge > .1 then
  if item.connector[2].value = "F1-Tap." then
   rem item.connector[2].alt = ""
   item.connector[2].value = "IT-.75."
   item.connector[2].locked = true
  Endif
 Endif
Endif



Rem Change Patern 25 Taps-------------------------

if item.cid = 25 then
 if item.insulation.status = "Off" or item.insulation.status = "Outside" then
  if item.connector[1].value = "F1-Tap." then
   rem item.connector[1].alt = ""
   item.connector[1].value = "OT-.625."
   item.connector[1].locked = true
  Endif
 Endif
Endif

if item.cid = 25 then
 if item.insulation.status = "Inside" and item.insulation.Gauge > .1 then
  if item.connector[1].value = "F1-Tap." then
   rem item.connector[1].alt = ""
   item.connector[1].value = "IT-.75."
   item.connector[1].locked = true
  Endif
 Endif
Endif

if item.cid = 25 then
 Dim Stretchout = (Item.dim["Width"].numvalue + Item.dim["Depth"].numvalue)*2
 if Item.dim["Length"].numvalue < 4 and Stretchout < 118 then
  item.option["Straight Type"].value = "1 Part Straight"
  Item.seam[1].value = "3/4 Spot."
  Endif
  if Item.dim["Length"].numvalue < 12 then
    item.Stiffener[1].value = "None"
    item.Stiffener[1].locked = true
   if item.wiregauge < 22 Then
     item.wiregauge = 22
   endif
 Endif
Endif


Rem --------------------------------------------------

Rem Pat 7 Connector Check
If item.cid = 7 Then
  If item.connector[2].value <> "OT-.625." and item.connector[2].value <> "IT-.75."  then
  Item.customdata["ChkFlag"].value = "Check Conn-2"
  endif
Endif

Rem Pat 15 Connector Check
If item.cid = 15 Then
  If item.connector[2].value <> "OT-.625." and item.connector[2].value <> "IT-.75."  then
  Item.customdata["ChkFlag"].value = "Check Conn-2"
  endif
Endif

Rem Pat 25 Connector Check
If item.cid = 25 Then
  If item.connector[1].value <> "OT-.625." and item.connector[1].value <> "IT-.75." then
  Item.customdata["ChkFlag"].value = "Check Conn-1"
  endif
Endif

item.update()