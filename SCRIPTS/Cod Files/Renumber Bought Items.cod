Dim Pat = item.cid
dim lp
for lp = 1 to item.dims
If item.boughtout = True then
	If Pat<>838 and Pat<>1049 and Pat<>149 and Pat<>1972 and Pat<>972 and Pat<>166 and Pat<>555 then  
		if item.dim[lp].name = "Diameter" then
		item.number = item.alias + "-" + item.dim["Diameter"].numvalue
		endif
		if item.dim[lp].name = "Width" then
			dim W1 = item.dim["Width"].numvalue
			dim D1 = item.dim["Depth"].numvalue
			item.number = item.alias + "-" + W1 + "x" + D1
		endif
	endif
endif
next lp