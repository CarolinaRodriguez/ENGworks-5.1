function ChangeRodLength()
    select item.CID
        case 838
            dim i
            for i =  1 to item.dims
                select item.Dim[i].Name
                    case "Rod Length"
                        item.Dim[i].Value = RoundLength(item.Dim[i].numValue)
                    case "Rod Extn Above"
                        item.Dim[i].Value = RoundLength(item.Dim[i].numValue)
                  Rem  case "Top Length"
                  Rem      item.Dim[i].Value = RoundLength(item.Dim[i].numValue)
                    case "Rod Length"
                        item.Dim[i].Value = RoundLength(item.Dim[i].numValue)
                end select
            next
        end select
end function

function RoundLength(length)
    return round(length)
end function

ChangeRodLength()

