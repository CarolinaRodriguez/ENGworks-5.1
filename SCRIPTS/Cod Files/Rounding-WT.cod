function Roundup(RndUp)
  dim RoundedUp = round(RndUp, 0)
  dim Result = round(RndUp,0)
  dim LeftOver = RoundedUp - Result
  if LeftOver > 0 then
    if LeftOver > 0.5 then
       Result = Result + 1
    else
       Result = Result + 0.5
  end if
  return Result
end function


function RoundToHalfInch(myLength)
  dim RoundedLength = round(myLength, 1)
  dim Result = round(myLength,0)
  dim LeftOver = RoundedLength - Result
  if LeftOver > 0 then
    if LeftOver > 0.5 then
       Result = Result + 1
    else
       Result = Result + 0.5
  end if
  return Result
end function
