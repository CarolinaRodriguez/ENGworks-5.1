requires task.selection

dim item1 = task.selection[1]
dim PANELNUMBER=inputbox("ENTER PANEL NUMBER & click ok", "PANEL NUMBER", item1.customdata["PANEL NUMBER"].value)


dim i = 1
dim jobitem

while i <= task.selection.count
    jobitem = task.selection[i]

  rem *** Changes happen here
    jobitem.customdata["PANEL NUMBER"].value=PANELNUMBER
  rem *** End of changes

  i = i + 1
end while